import itertools
def solve():
    L='SENDMORY'
    for p in itertools.permutations('0123456789',8):
        m=dict(zip(L,p))
        if m['S']=='0' or m['M']=='0': continue
        S,E,N,D,M,O,R,Y=[int(m[c]) for c in L]
        if 1000*S+100*E+10*N+D + 1000*M+100*O+10*R+E == 10000*M+1000*O+100*N+10*E+Y:
            return m
print(solve())
